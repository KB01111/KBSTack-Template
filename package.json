{
  "name": "kbstack",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev:web": "turbo dev --filter=next",
    "dev:ios": "turbo dev --filter=expo",
    "dev:windows": "turbo dev --filter=desktop",
    "dev:all": "turbo dev",
    "build:web": "turbo build --filter=next",
    "build:ios": "turbo build --filter=expo",
    "build:windows": "turbo build --filter=desktop",
    "build:all": "turbo build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  }
}
